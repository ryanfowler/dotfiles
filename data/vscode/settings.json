{
  "breadcrumbs.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.cursorBlinking": "solid",
  "editor.fontFamily": "'Monaspace Neon', 'Source Code Pro', 'Ubuntu Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 15,
  "editor.guides.indentation": false,
  "editor.inlayHints.enabled": "off",
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    80
  ],
  "editor.tokenColorCustomizations": {
    "comments": "#7e8a89"
  },
  "explorer.confirmDragAndDrop": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.coverprofile": true,
    "**/*.out": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/vendor/**": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "github.copilot.enable": {
    "*": false
  },
  "go.buildTags": "ceph,e2e,integration,sandbox,smoke",
  "gopls": {
    "ui.semanticTokens": true
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "protoc": {
    "options": [
      "--proto_path=${env.GOPATH}/src"
    ]
  },
  "redhat.telemetry.enabled": false,
  "remote.autoForwardPorts": false,
  "rust-analyzer.cargo.features": "all",
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.check.features": "all",
  "rust-analyzer.inlayHints.maxLength": 10,
  "search.exclude": {
    "**/bower_components": true,
    "**/node_modules": true,
    "**/vendor": true
  },
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.lineHeight": 1.2,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.hlsearch": true,
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-d>"],
      "after": ["<C-d>", "z", "z"]
    },
    {
      "before": ["<C-u>"],
      "after": ["<C-u>", "z", "z"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.referenceSearch.trigger"]
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["g", "D"],
      "commands": ["editor.action.revealDeclaration"]
    },
    {
      "before": ["g", "I"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "t"],
      "commands": ["editor.action.goToTypeDefinition"]
    },
    {
      "before": ["<leader>", "r", "n"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.codeAction"]
    },
    {
      "before": ["<leader>", "<leader>"],
      "commands": ["workbench.action.showAllEditors"]
    },
    {
      "before": ["<leader>", "g", "L"],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "Open Permalink"
        }
      ]
    },
    {
      "before": ["<leader>", "g", "l"],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "Copy Permalink"
        }
      ]
    },
    {
      "before": ["<leader>", "g", "B"],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "Open Blame Permalink"
        }
      ]
    },
    {
      "before": ["<leader>", "g", "b"],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "Copy Blame Permalink"
        }
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "g", "L"],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "Open Permalink"
        }
      ]
    },
    {
      "before": ["<leader>", "g", "l"],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "Copy Permalink"
        }
      ]
    },
    {
      "before": ["<leader>", "g", "B"],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "Open Blame Permalink"
        }
      ]
    },
    {
      "before": ["<leader>", "g", "b"],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "Copy Blame Permalink"
        }
      ]
    }
  ],
  "vim.useSystemClipboard": true,
  "window.nativeTabs": true,
  "window.titleBarStyle": "custom",
  "workbench.colorTheme": "GitHub Dark Colorblind (Beta)",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 8,
  "workbench.editor.showTabs": "multiple",
  "[go]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.formatOnSave": true,
    "editor.snippetSuggestions": "none",
    "editor.tabSize": 8
  },
  "[go.mod]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "[rust]": {
    "editor.formatOnSave": true
  }
}
